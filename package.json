{
  "name": "factory-t",
  "version": "0.1.1",
  "description": "",
  "main": "build/factory-t.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "tslint": "tslint -t stylish './src/**/*.ts'",
    "build": "rm -rf ./build && tsc --build src",
    "examples:build": "jdi ./src/factory-t.examples.test.ts && markdown-toc -i ./src/factory-t.examples.test.ts.md",
    "examples:build:watch": "sane 'npm run examples:build' ./src --glob='*examples.test.ts'",
    "pre-push": "run-p -ln test tslint",
    "pre-commit": "run-s -ln tslint",
    "commitlint": "commitlint",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run pre-push",
      "pre-commit": "npm run pre-commit",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "author": "Maxim Rodionov",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "7.5.2",
    "@commitlint/config-conventional": "7.5.0",
    "@types/jest": "24.0.0",
    "conventional-changelog-cli": "2.0.12",
    "cz-conventional-changelog": "2.1.0",
    "husky": "1.3.1",
    "jdi": "1.1.3",
    "jest": "23.6.0",
    "markdown-toc": "1.2.0",
    "npm-run-all": "4.1.5",
    "sane": "4.1.0",
    "ts-jest": "23.10.5",
    "tslint": "5.12.0",
    "tslint-consistent-codestyle": "1.15.0",
    "tslint-eslint-rules": "5.4.0",
    "typescript": "3.2.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {}
}
